#include "Ship.h"

typedef struct Gameboard Gameboard;

struct Gameboard {
	Ship* ships[10];
	int shipCount;
};
Gameboard* new_Gameboard();
void drawBoard(Gameboard* gameboard);
void addShip(Gameboard* gameboard, int x, int y);
void tryToMoveShipTo(Gameboard* gameboard, Ship ship, int x, int y);


