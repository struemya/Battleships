void tryToMoveShipTo(Ship ship, int x, int y){
	if(originDistance(ship,x,y)==1) {
		int i;
		bool overlapping;

		for(i=0,i<shipCount,i++){
			overlapping = isOverlapping(ships[i],ship);
		}
	}
}
